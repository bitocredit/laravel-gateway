{
  "name": "bitocredit/laravel-gateway",
  "description": "A Laravel package for integrate with bitocredit API.",
  "license": "MIT",
  "keywords": [
    "Bitocredit",
    "API",
    "currency"
  ],
  "autoload": {
    "psr-4": {
      "Bitocredit\\LaravelGateway\\": "src/"
    }
  },
  "authors": [
    {
      "name": "reza motahary",
      "email": "reza.rz00980pp@gmail.com"
    }
  ],
  "require": {
    "bitocredit/php-gateway": "*"
  },
  "autoload-dev": {
    "psr-4": {
      "Bitocredit\\LaravelGateway\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bitocredit\\LaravelGateway\\LaravelGatewayServiceProvider"
      ],
      "aliases": {
        "Gateway": "Bitocredit\\LaravelGateway\\Facade"
      }
    }
  }
}
